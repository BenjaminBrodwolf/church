{
  "name": "church",
  "version": "1.0.0",
  "description": "Church encoding utility library for JavaScript",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "benchmark": "node benchmark",
    "build": "rm -rf dist && mkdir dist && rollup src/index.js | babel --presets es2015 -o dist/index.js",
    "docs": "docco src/*.js",
    "prepublish": "npm run test && npm run build && npm run docs",
    "test": "standard && babel-node node_modules/.bin/tape 'test/**/*'",
    "watch-docs": "nodemon --exec npm run docs",
    "watch-test": "nodemon --exec npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benji6/church.git"
  },
  "keywords": [
    "alonzo",
    "boolean",
    "calculus",
    "car",
    "cdr",
    "church",
    "combinatory",
    "cons",
    "curry",
    "encoding",
    "functional",
    "haskell",
    "lambda",
    "lisp",
    "list",
    "logic",
    "numeral",
    "pair",
    "predicate"
  ],
  "author": "Ben Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benji6/church/issues"
  },
  "homepage": "https://github.com/benji6/church#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.5",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "benchmark": "^2.0.0",
    "docco": "^0.7.0",
    "nodemon": "^1.8.1",
    "rollup": "^0.25.1",
    "standard": "^5.4.1",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "combinators-js": "^2.2.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist"
    ]
  }
}
